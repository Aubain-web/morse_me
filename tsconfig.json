{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outFile": "../../built/local/tsc.js",
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["**/*.ts"],
  "exclude": ["**/*.spec.ts"]
}
